<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>{{ title }}</title>
  <link rel="self" type="application/atom+xml" href="{{ craft.request.url }}" />
  <link rel="alternate" type="text/html" href="{{ altUrl }}" />
  <id>{{ craft.request.url }}</id>
  <updated>{{ now.atom }}</updated>
  <icon>{{ siteUrl }}images/favicon.ico</icon>

  {% block content %}{% endblock %}

</feed>
